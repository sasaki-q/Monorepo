{
    "compilerOptions": {
        "target": "es2019",
        "module": "commonjs",
        "strict": true,
        "sourceMap": true,
        "composite": true,
        "incremental": true,
        "declaration": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": true
    }
}